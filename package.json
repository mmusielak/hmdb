{
    "name": "hmdb",
    "description": "A bunch of scripts for cataloging home movie collection",
    "author": "Mikolaj Musielak",
    "license": "MIT",
    "version": "0.1.0",
    "main": "index.ts",
    "type": "module",
    "scripts": {
        "install": "node install.mjs",
        "build": "node --title=hmdb --experimental-transform-types --use-largepages=silent --no-warnings web/build-web.ts",
        "start": "node --title=hmdb --experimental-transform-types --use-largepages=silent --no-warnings index.ts",
        "test": "node --title=hmdb --experimental-transform-types --use-largepages=silent --no-warnings --test"
    },
    "devDependencies": {
        "@types/node": "^22.13.10"
    }
}
