{
    "name": "hmdb",
    "description": "A bunch of scripts for cataloging home movie collection",
    "author": "Mikolaj Musielak",
    "license": "MIT",
    "version": "0.1.0",
    "main": "index.js",
    "type": "module",
    "scripts": {
        "install": "node install.js",
        "build": "node --title=hmdb --experimental-transform-types --use-largepages=silent --no-warnings web/build-web.js",
